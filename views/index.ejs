<li>*profil</li>
<li>*avatar</li>
<li>*baner</li>
<li>*tavsiye</li>
<li>*sicil</li>
<li>*öneri</li>
<li>*bug</li>
<li>*davet</li>
<li>*destek</li>
  </ul>
</div>
<div id="fun-box" class="commands w-80">
<h4 class="title">Eğlence</h4>
<button onclick="fun()" id="fun-btn" class="command-btn">+</button>
<ul id="fun" class="command w-80">
<li>*sarıl</li>
<li>*öp</li>
<li>*öldür</li>
<li>*aşk-ölçer</li>
<li>*reverse</li>
<li>*dosya-oluştur</li>
  </ul>
</div>
<div id="rp-box" class="commands w-80">
<h4 class="title">Roleplay</h4>
<button onclick="rp()" id="rp-btn" class="command-btn">+</button>
